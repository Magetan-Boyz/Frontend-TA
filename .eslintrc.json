{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "extends": ["eslint:recommended", "plugin:prettier/recommended"],
  "plugins": ["prettier", "filename-rules", "json"],
  "rules": {
    "semi": ["error", "always"],
    "indent": ["error", 2, { "SwitchCase": 1 }],
    "camelcase": ["error", { "properties": "never" }],
    "filename-rules/match": [2, { "^(?!).*\\.js$": "camelCase" }]
  }
}
